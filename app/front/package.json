{
  "name": "front",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "homepage": "",
  "scripts": {
    "dev": "vite --mode development",
    "build": "tsc -b && vite build",
    "build:prod": "pnpm build",
    "types": "tsc --project tsconfig.api.json --noEmit",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "format": "prettier --write \"src/**/*.{ts,tsx,js,jsx,json}\"",
    "fix:all": "pnpm format && pnpm lint:fix",
    "start:prod": "vite preview --port 8080 --host",
    "gen:srv": "bash ./gen-srv.sh"
  },
  "dependencies": {
    "@hookform/resolvers": "^5.0.1",
    "@mantine/charts": "^8.3.5",
    "@mantine/core": "^8.3.2",
    "@mantine/dates": "^8.3.2",
    "@mantine/dropzone": "^8.3.5",
    "@mantine/hooks": "^8.3.2",
    "@mantine/notifications": "^8.3.2",
    "@remix-run/router": "^1.16.1",
    "@tabler/icons-react": "3.32.0",
    "dotenv": "^16.4.7",
    "fp-ts": "^2.16.9",
    "io-ts": "^2.2.21",
    "io-ts-types": "^0.5.19",
    "lodash": "^4.17.21",
    "mobx": "^6.13.5",
    "mobx-react-lite": "^4.1.0",
    "mobx-utils": "^6.1.0",
    "path": "^0.12.7",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-hook-form": "^7.56.1",
    "react-router-dom": "^6.23.1",
    "sass": "1.77.6",
    "sass-loader": "12.6.0",
    "style-loader": "3.3.4",
    "zod": "^3.24.3"
  },
  "devDependencies": {
    "@eslint/js": "^9.15.0",
    "@openapitools/openapi-generator-cli": "^2.20.0",
    "@trivago/prettier-plugin-sort-imports": "^5.2.2",
    "@types/lodash": "^4.17.13",
    "@types/node": "^22.10.1",
    "@types/react": "^18.3.12",
    "@types/react-dom": "^18.3.1",
    "@typescript-eslint/parser": "^8.17.0",
    "@vitejs/plugin-react": "^4.3.4",
    "eslint": "^9.15.0",
    "eslint-import-resolver-typescript": "^3.7.0",
    "eslint-plugin-import": "^2.31.0",
    "eslint-plugin-react": "^7.37.2",
    "eslint-plugin-react-hooks": "^5.0.0",
    "eslint-plugin-react-refresh": "^0.4.14",
    "globals": "^15.12.0",
    "lint-staged": "^15.2.10",
    "prettier": "^3.4.1",
    "stylelint": "^16.11.0",
    "stylelint-config-standard-scss": "^14.0.0",
    "stylelint-order": "^6.0.4",
    "typescript": "~5.6.2",
    "typescript-eslint": "^8.15.0",
    "typescript-plugin-css-modules": "^5.1.0",
    "vite": "^6.0.1"
  },
  "lint-staged": {
    "src/**/*.{js,ts,tsx,json,md}": [
      "eslint --config eslintrc.js --fix",
      "prettier --write"
    ],
    "src/**/*.{css,scss}": [
      "stylelint --fix",
      "prettier --write"
    ]
  }
}
